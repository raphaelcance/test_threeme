\begin_layout Subsection*
Demand for input in activity a:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\Delta k_{a,t}^{n} & =\Delta y_{a,t}-\Delta prog_{a,t}^{K}+\Delta SUBST\_K{}_{a,t}\\
\Delta SUBST\_K_{a,t}^{n} & =-\eta_{a}^{KL}\varphi_{a,t-1}^{L}\Delta(c{}_{a,t}^{K}-c_{j,t}^{L})-\eta_{a}^{KE}\varphi_{a,t-1}^{E}\Delta(c_{a,t}^{K}-p_{a,t}^{E})-\eta_{a}^{KMat}\varphi_{a,t-1}^{Mat}\Delta(c_{a,t}^{K}-p_{a,t}^{Mat})\nonumber 
\end{align}

\end_inset

 
\begin_inset Formula 
\begin{align}
\Delta l_{a,t}^{n} & =\Delta y_{a,t}-\Delta prog_{a,t}^{L}+\Delta SUBST\_L_{a,t}\\
\Delta SUBST\_L_{a,t}^{n} & =-\eta_{a}^{KL}\varphi_{a,t-1}^{K}\Delta(c_{a,t}^{L}-c_{a,t}^{K})-\eta_{a}^{LE}\varphi_{a,t-1}^{E}\Delta(c_{a,t}^{L}-p_{a,t}^{E})-\eta_{j}^{LM}\varphi_{a,t-1}^{Mat}\Delta(c_{a,t}^{L}-p_{a,t}^{Mat})\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Assuming that the adjustment process is defined according to Equations 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:Adjust Notional"

\end_inset

, 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:Adjust Expect"

\end_inset

 and 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:Adjust Substi"

\end_inset

, the full dynamic for labor is also defined by the three following additional
 relations: 
\begin_inset Formula 
\begin{align*}
ln(L_{a,t}) & =\lambda_{0}^{L}.ln(L{}_{a,t}^{n})+(1-\lambda_{0}^{L})ln(L_{a,t-1}+\Delta ln(L_{a,t}^{e}))\\
\Delta ln(L_{a,t}^{e}) & =\lambda_{1}^{L}.\Delta ln(L_{a,t-1}^{e})+\lambda_{2}^{L}.\Delta ln(L_{a,t-1})+\lambda_{3}^{L}.\Delta ln(L_{a,t}^{n})+\lambda_{4}^{L}.\Delta ln(L_{a,t+1})\\
SUBST\_L_{a,t} & =\lambda_{5}^{L}.SUBST\_L{}_{a,t}^{n}+(1-\lambda_{5}^{L}).SUBST\_L{}_{a,t-1}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
For the sake of concision, the representation of adjustment dynamic [Equations
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:Adjust Notional"

\end_inset

,
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:Adjust Expect"

\end_inset

 and
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:Adjust Substi"

\end_inset

] is not reproduced for each variable.
 Only notional variables are presented in the rest of the document.
 
\begin_inset Formula 
\begin{align}
\Delta e_{a,t}^{n} & =\Delta y_{a,t}-\Delta prog_{a,t}^{E}+\Delta SUBST\_E_{a,t}\\
\Delta SUBST\_E_{a,t}^{n} & =-\eta_{a}^{KE}\varphi_{a,t-1}^{K}\Delta(p_{a,t}^{E}-c_{a,t}^{K})-\eta_{a}^{LE}\varphi_{a,t-1}^{L}\Delta(p_{a,t}^{E}-c_{a,t}^{L})-\eta_{a}^{EMat}\varphi_{a,t-1}^{Mat}\Delta(p_{a,t}^{E}-p_{a,t}^{Mat})\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\Delta mat_{a,t}^{n} & =\Delta y_{a,t}-\Delta prog_{a,t}^{Mat}+\Delta SUBST\_Mat_{a,t}\\
\Delta SUBST\_Mat_{a,t}^{n} & =-\eta_{j}^{KLMat}\varphi_{a,t-1}^{K}\Delta(p_{a,t}^{Mat}-c_{a,t}^{K})-\eta_{a}^{LMat}\varphi_{a,t-1}^{L}\Delta(p_{a,t}^{Mat}-c_{a,t}^{L})\nonumber \\
 & \qquad-\eta_{a}^{EMat}\varphi_{a,t-1}^{E}\Delta(p_{a,t}^{E}-p_{a,t}^{Mat})\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Standard
with 
\begin_inset Formula $\varphi_{a}^{j}=\frac{P_{j,a}^{Input}I_{j,a}^{Input}}{\sum_{j}P_{j,a}^{Input}I_{j,a}^{Input}}$
\end_inset

