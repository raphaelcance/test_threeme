\nonumber \\
\sum_{k'}\varphi_{h,k,k'}^{REHAB} & = & 1
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Subsection*
The user cost of building rehabilitation
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
UC_{h,k}^{REHAB} & = & UC_{h,k}^{K\_REHAB}+UC_{h,k}^{E\_REHAB}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
UC_{h,k}^{E\_REHAB} & = & \sum_{k'=k+1}^{K}\varphi_{h,k,k'}^{REHAB}.UC_{h,k'}^{E}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
UC_{h,k} & = & UC_{h,k}^{K}+UC_{h,k}^{E}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
UC_{h,k}^{K\_REHAB} & = & P_{h,k}^{REHAB\_\delta^{BUIL}}(R_{h,k}^{CASH\_REHAB}+\frac{R_{h,k}^{LOAN\_REHAB}R_{h,k,t-1}^{I\_REHAB}LD_{h,k}^{REHAB}}{1-(1+R_{h,k,t-1}^{BUIL\_REHAB})^{-LD_{h,k}^{REHAB}}})\\
R_{h,k}^{LOAN\_REHAB} & = & 1-R_{h,k}^{CASH\_REHAB}\\
LD_{h,k}^{REHAB} & \leqq & 1/\delta_{h,k}^{REHAB}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
UC_{h,k}^{K} & = & P_{h,k,k}^{REHAB}\delta_{h,k}^{BUIL}(R_{h,k}^{CASH}+\frac{R_{h,k}^{LOAN}R_{h,k,t-1}^{I\_BUIL}LD_{h,k}}{1-(1+R_{h,k,t-1}^{I\_BUIL})^{-LD_{h,k}}})\\
R_{h,k}^{LOAN} & = & 1-R_{h,k}^{CASH}\\
LD_{h,k} & \leqq & 1/\delta_{h,k}^{BUIL}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\delta_{h,k}^{REHAB}=\sum_{k'=k+1}^{K}\varphi_{h,k,k'}^{REHAB}\delta_{h,k'}^{BUIL}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\delta_{h,k}^{BUIL}=\sum_{k'=0}^{k-1}\delta_{h,k,k'}^{BUIL}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
UC_{h,k}^{E} & = & \frac{\left(1+\overset{\bullet}{P}_{h,k}^{Ener\_m\text{²\_e}}\right)^{1/\delta_{h,k}^{BUIL}}-1}{\overset{\bullet}{P}_{h,k}^{Ener\_m\text{²}\_e}/\delta_{h,k}^{BUIL}}.P_{h,k}^{Ener\_m^{2}}\\
P_{h,k}^{Ener\_m\text{²}}.BUIL_{h,k} & = & PENER^{BUIL}.ENER_{h,k}\\
\overset{\bullet}{P}_{h,k,t}^{Ener\_m\text{²}\_e} & = & \lambda_{0}^{Ener\_BUIL}\overset{\bullet}{P}_{h,k,t-1}^{Ener\_m\text{²}\_e}+\lambda_{1}^{Ener\_BUIL}\overset{\bullet}{P}_{h,k,t-1}^{Ener\_m\text{²}}
\end{eqnarray}

\end_inset


\end_layout

