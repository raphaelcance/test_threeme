\begin_layout Subsection*
Introduction des écarts de prix relatifs en volume dans les équations de
 substitutions inter-énergie des ménages :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\begin{array}{ccc}
\Delta c_{22\_BUIL^{eff}}= & \Delta buil\_fuel+\Delta standard\_BUIL+\eta^{C_{22\_Buil^{eff}}}.(\Delta pc_{22}-\Delta ph)\\
 & +\eta^{C_{Buil^{eff}\_22\_24}}.\left(\frac{PC_{24,t-1}^{TEP}}{PC_{22,t-1}^{TEP}}.\Delta pc_{24}-\frac{PC_{22,t-1}^{TEP}}{PC_{24,t-1}^{TEP}}.\Delta pc_{22}\right)\frac{C_{24\_BUIL\_eff,t-1}}{C_{22\_BUIL\_eff,_{t-1}}+C_{24\_BUIL\_eff,t-1}} & if\; BUIL\_fuel>0
\end{array}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\begin{array}{c}
\Delta c_{22\_BUIL^{eff}}=\Delta buil\_fuel+\Delta standard\_BUIL\; if\; BUIL\_fuel\eqslantless0\\
\\
\end{array}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\begin{array}{ccc}
\Delta c_{23\_BUIL^{eff}}= & \Delta buil\_fuel+\Delta standard\_BUIL+\eta^{C_{23\_Buil^{eff}}}.(\Delta pc_{23}-\Delta ph)\\
 & +\eta^{C_{Buil^{eff}\_24\_23}}.\left(\frac{PC_{24,t-1}^{TEP}}{PC_{23,t-1}^{TEP}}.\Delta pc_{24}-\frac{PC_{23,t-1}^{TEP}}{PC_{24,t-1}^{TEP}}.\Delta pc_{23}\right)\frac{C_{24\_BUIL\_eff,t-1}}{C_{23\_BUIL\_eff,_{t-1}}+C_{24\_BUIL\_eff,t-1}} & if\; BUIL\_fuel>0
\end{array}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\begin{array}{c}
\Delta c_{23\_BUIL^{eff}}=\Delta buil\_fuel+\Delta standard\_BUIL\; if\; BUIL\_fuel\eqslantless0\\
\\
\end{array}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\begin{array}{ccc}
\Delta c_{24\_BUIL^{eff}}= & \Delta buil\_fuel+\Delta standard\_BUIL+\eta^{C_{243\_Buil^{eff}}}.(\Delta pc_{24}-\Delta ph)\\
 & +\eta^{C_{Buil^{eff}\_22\_24}}.\left(\frac{PC_{22,t-1}^{TEP}}{PC_{24,t-1}^{TEP}}.\Delta pc_{22}-\frac{PC_{24,t-1}^{TEP}}{PC_{22,t-1}^{TEP}}.\Delta pc_{22}\right)\frac{C_{22\_BUIL\_eff,t-1}}{C_{23\_BUIL\_eff,_{t-1}}+C_{24\_BUIL\_eff,t-1}} & if\; BUIL\_fuel>0
\end{array}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\begin{array}{cc}
\Delta c_{24\_BUIL^{eff}}=\Delta buil\_fuel+\Delta standard\_BUIL+\eta^{C_{243\_Buil^{eff}}}.(\Delta pc_{24}-\Delta ph)\\
+\eta^{C_{Buil^{eff}\_22\_24}}.\left(\frac{PC_{22,t-1}^{TEP}}{PC_{24,t-1}^{TEP}}.\Delta pc_{22}-\frac{PC_{24,t-1}^{TEP}}{PC_{22,t-1}^{TEP}}.\Delta pc_{22}\right)\frac{C_{22\_BUIL\_eff,t-1}}{C_{23\_BUIL\_eff,_{t-1}}+C_{24\_BUIL\_eff,t-1}}\\
+\eta^{C_{Buil^{eff}\_24\_23}}.\left(\frac{PC_{23,t-1}^{TEP}}{PC_{24,t-1}^{TEP}}.\Delta pc_{23}-\frac{PC_{24,t-1}^{TEP}}{PC_{23,t-1}^{TEP}}.\Delta pc_{24}\right)\frac{C_{23\_BUIL\_eff,t-1}}{C_{23\_BUIL\_eff,_{t-1}}+C_{24\_BUIL\_eff,t-1}} & if\; BUIL\_fuel>0
