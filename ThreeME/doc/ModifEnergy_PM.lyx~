\begin_body

\begin_layout Section*
Ajout des équations de CSPE:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
CSPE=CSPE\_elec+CSPE\_heat+CSPE\_biocarb
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
CSPE\_elec=CSPE\_elec_{2305}+CSPE\_elec_{2306}+CSPE\_elec_{2307}.\frac{Q\_Mtep_{2307,t}-Q\_Mtep_{2307,0}}{Q\_Mtep_{2307,t}}+CSPE\_elec_{2308}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
CSPE\_heat_{a}=\left(CU_{a}-CU_{2401}\right).Y_{a}\;\; for\; a={2402,2403,2404,2405,2406}>0
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
CSPE\_heat=CSPE\_heat_{2402}.\left(\frac{Q\_Mtep_{2302,t}-Q\_Mtep_{2302,0}}{Q\_Mtep_{2302,t}}\right)+CSPE\_heat_{2403}+CSPE\_heat_{2404}+CSPE\_heat_{2405}+CSPE\_heat_{2406}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
CU_{23}^{foss}=\frac{\sum_{a}CU_{a}.Y_{a}}{\sum_{a}Y_{a}}\;\; for\; a={2301,2302,2303,2304}>0
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
CSPE\_biocarb=\left(CU_{2202}-CU_{2201}\right).Y_{2202}
\end{equation}

\end_inset


\end_layout

\begin_layout Subsection*
Energetic Household's Consumption in Mtep :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\Delta q\_Mtep\_H_{e,h}=\Delta(ENER\_BUIL_{e,h})
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\Delta q\_Mtep\_TRSP_{e,h}=\Delta(ENER\_AUTO_{e,h})
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
Q\_Mtep\_H_{e}=\sum_{h}Q\_Mtep\_H_{e,h}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
Q\_Mtep\_TRSP_{e}=\sum_{h}Q\_Mtep\_TRSP_{e,h}
\end{equation}

\end_inset


\end_layout

\begin_layout Subsection*
Energetic Production in Mtep :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\Delta q\_Mtep_{c}=\Delta(q_{c}+m_{c}-x_{c})
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\Delta q\_Mtep_{e,a}=\Delta e_{e,a}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
Q\_Mtep_{e}=Q\_Mtep\_TRSP_{e}+Q\_Mtep\_H_{e}
\end{equation}

\end_inset


\end_layout

\begin_layout Subsection*
Energetic consumption of automobile by household:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\Delta q\_Mtep\_autoparc_{h}=\Delta ch_{03,h}
\end{equation}

\end_inset


\end_layout

\begin_layout Subsection*
Energetic Production in Mtep by subsectors:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\Delta q\_Mtep\_autoparc,h=\Delta ch_{03,h}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
Q\_Mtep_{22}=\varphi_{22subsec}^{Y}.(Q\_Mtep\_H_{22}+Q\_Mtep\_TRSP_{22}+Q\_Mtep_{22,a})
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
Q\_Mtep_{23}=\varphi_{23subsec}^{Y}.(Q\_Mtep\_H_{23}+Q\_Mtep_{23,a})
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
Q\_Mtep_{24}=\varphi_{24subsec}^{Y}.(Q\_Mtep\_H_{24}+Q\_Mtep_{24,a})
\end{equation}

\end_inset


\end_layout

\begin_layout Subsection*
Conversion between primary energy and final energy:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
Q\_Mtep_{e}^{EP}=\zeta_{e}^{ENE}.Q\_Mtep_{e}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Note Note
status open

\begin_layout Plain Layout
PM: j'ai mis ici sous sa forme générale alors que dans les équations de
 Gael cela ne concerne que le nucléaire mais il suffira de calibrer le facteur
 égale à 1 pour retrouver les résultats pour les autres types d'énérgie.
 
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
Q\_Mtep=\sum_{subsec}Q\_Mtep_{subsec}+\sum_{a}Q\_Mtep_{a}+\sum_{e}Q\_Mtep\_H\_TRSP_{e}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
Q\_Mtep^{EP}=\sum_{e}\zeta_{e}^{ENE}.Q\_Mtep_{e}
\end{equation}

\end_inset


\end_layout

\begin_layout Subsection*
Unitary energy prices in euro per Mtep :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
PE_{e}^{TEP}.Q\_Mtep_{a,e}=PE_{e}.E_{e,a}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
PCH_{e}^{TEP}.Q\_Mtep\_H_{e}=\sum_{k}PENER\_BUIL_{k,e}.PE_{e}e.E_{e,a}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
